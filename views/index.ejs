<!DOCTYPE HTML>
<head>
</head>
<body>
  TWATTER GADUNGAN JADI-JADIAN
  <br>
  TIMELINE
  <% for (let j in timeline){ %>
    <li><%= timeline[j].user.name %>: <%= timeline[j].text %>
      <% if (timeline[j].entities.hasOwnProperty('media')) { %>
        <img src=<%= timeline[j].entities.media[0].media_url %> height="60px">
      <% } %>
  <% } %>
  <br>
  <br>
  SILAHKAN SEARCH
  <form action="/" method="POST">
    <input type="text" name="q">
    <input type="submit">
  </form>
  <br>
  <% if (data){
    for (let i in data.statuses){ %>
      <li>Username: <%= data.statuses[i].user.name %> ||| Twit: <%= data.statuses[i].text %>
    <% }
  } %>

  <br>
  Tweet dari Sini
  <form action="/tweet" method="POST">
    <input type ="text" name = "tweetbaru">
    <input type="submit" value="Post Tweet">
  </form>
</body>
</html>
